{
  "name": "subscription-management",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' sm-app.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/db/data-source.ts",
    "migration:create:public": "sh -c 'typeorm migration:create ./src/db/migrations/public/$0'",
    "migration:create:tenant": "sh -c 'typeorm migration:create ./src/db/migrations/tenant/$0'",
    "db:migrate": "npm run typeorm -- migration:run",
    "db:migrate:revert": "npm run typeorm -- migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express-joi-validation": "^6.0.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.23",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
